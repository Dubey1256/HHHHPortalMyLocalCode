{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "service-portfolio-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/servicePortfolio/ServicePortfolioWebPart.js",
          "manifest": "./src/webparts/servicePortfolio/ServicePortfolioWebPart.manifest.json"
        }
      ]
    },
    "component-portfolio-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/componentPortfolio/ComponentPortfolioWebPart.js",
          "manifest": "./src/webparts/componentPortfolio/ComponentPortfolioWebPart.manifest.json"
        }
      ]
    },
    "component-profile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/componentProfile/ComponentProfileWebPart.js",
          "manifest": "./src/webparts/componentProfile/ComponentProfileWebPart.manifest.json"
        }
      ]
    },
    "component-edit-popup-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/componentEditPopup/ComponentEditPopupWebPart.js",
          "manifest": "./src/webparts/componentEditPopup/ComponentEditPopupWebPart.manifest.json"
        }
      ]
    },
    "task-user-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskUserManagement/TaskUserManagementWebPart.js",
          "manifest": "./src/webparts/taskUserManagement/TaskUserManagementWebPart.manifest.json"
        }
      ]
    },
    "task-dashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskDashboard/TaskDashboardWebPart.js",
          "manifest": "./src/webparts/taskDashboard/TaskDashboardWebPart.manifest.json"
        }
      ]
    },
    "taskprofile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskprofile/TaskprofileWebPart.js",
          "manifest": "./src/webparts/taskprofile/TaskprofileWebPart.manifest.json"
        }
      ]
    },
    "component-comments-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/componentComments/ComponentCommentsWebPart.js",
          "manifest": "./src/webparts/componentComments/ComponentCommentsWebPart.manifest.json"
        }
      ]
    },
    "task-steps-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskSteps/TaskStepsWebPart.js",
          "manifest": "./src/webparts/taskSteps/TaskStepsWebPart.manifest.json"
        }
      ]
    },
    "hr-tool-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hrTool/HrToolWebPart.js",
          "manifest": "./src/webparts/hrTool/HrToolWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap-css": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css",
      "globalName": "Common-css"
    },
    "bootstrap-js": {
      "path": "https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js",
      "globalName": "common-js"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js Jump ",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "ServicePortfolioWebPartStrings": "lib/webparts/servicePortfolio/loc/{locale}.js",
    "ComponentPortfolioWebPartStrings": "lib/webparts/componentPortfolio/loc/{locale}.js",
    "ComponentProfileWebPartStrings": "lib/webparts/componentProfile/loc/{locale}.js",
    "ComponentEditPopupWebPartStrings": "lib/webparts/componentEditPopup/loc/{locale}.js",
    "TaskUserManagementWebPartStrings": "lib/webparts/taskUserManagement/loc/{locale}.js",
    "TaskDashboardWebPartStrings": "lib/webparts/taskDashboard/loc/{locale}.js",
    "TaskprofileWebPartStrings": "lib/webparts/taskprofile/loc/{locale}.js",
    "ComponentCommentsWebPartStrings": "lib/webparts/componentComments/loc/{locale}.js",
    "TaskStepsWebPartStrings": "lib/webparts/taskSteps/loc/{locale}.js",
    "HrToolWebPartStrings": "lib/webparts/hrTool/loc/{locale}.js"
  }
}
